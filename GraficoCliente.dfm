  object qCliente: TADOQuery
    Connection = TDMPrincipal.Connection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT TOP(10) C.COD_CLI,C.NOME_CLI,SUM(CP.VALORTOTAL_COMPRA)AS ' +
        #39'Valor Total'#39
      'FROM CLIENTE C'
      'INNER JOIN COMPRA CP ON CP.COD_CLI = C.COD_CLI'
      'GROUP BY C.COD_CLI,C.NOME_CLI'
      'ORDER BY [Valor Total] ASC')
