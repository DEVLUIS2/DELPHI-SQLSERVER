  object qFornecedor: TADOQuery
    Connection = TDMPrincipal.Connection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT C.NOME_CLI,SUM(CP.QUANTIDADE_COMPRA)AS '#39'QUNATIDADE DE PRO' +
        'DUTOS DA COMPRA'#39
      'FROM CLIENTE C'
      'INNER JOIN COMPRA CP ON CP.COD_CLI = C.COD_CLI'
      'GROUP BY C.NOME_CLI'
      'ORDER BY [QUNATIDADE DE PRODUTOS DA COMPRA] DESC')
    Left = 40
    Top = 344
    object qFornecedorNOME_CLI: TStringField
      FieldName = 'NOME_CLI'
      Size = 100
    end
    object qFornecedorQUNATIDADEDEPRODUTOSDACOMPRA: TIntegerField
      FieldName = 'QUNATIDADE DE PRODUTOS DA COMPRA'
      ReadOnly = True
    end
  end
