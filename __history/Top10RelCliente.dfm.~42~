  end
  object qTopCliente: TADOQuery
    Connection = TDMPrincipal.Connection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT TOP(10) C.COD_CLI,C.NOME_CLI,C.RG_CLI,CP.VALORTOTAL_COMPR' +
        'A,CP.QUANTIDADE_COMPRA'
      'FROM CLIENTE C'
      'INNER JOIN COMPRA CP ON CP.COD_CLI = C.COD_CLI')
    Left = 112
    Top = 328
    object qTopClienteCOD_CLI: TAutoIncField
      FieldName = 'COD_CLI'
      ReadOnly = True
    end
    object qTopClienteNOME_CLI: TStringField
      FieldName = 'NOME_CLI'
      Size = 100
    end
    object qTopClienteRG_CLI: TStringField
      FieldName = 'RG_CLI'
      Size = 12
    end
    object qTopClienteVALORTOTAL_COMPRA: TBCDField
      FieldName = 'VALORTOTAL_COMPRA'
      Precision = 15
      Size = 2
    end
    object qTopClienteQUANTIDADE_COMPRA: TIntegerField
      FieldName = 'QUANTIDADE_COMPRA'
    end
  end
  object dsTopCliente: TDataSource
    DataSet = qTopCliente
    Left = 192
    Top = 328
  end
end
