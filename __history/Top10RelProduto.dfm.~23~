  object dsTopProduto: TDataSource
    DataSet = qTopProduto
    Left = 216
    Top = 312
  end
  object qTopProduto: TADOQuery
    Connection = TDMPrincipal.Connection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT TOP 10 SUM(CI.QUANTIDADE_ITENS) AS '#39'Produtos Comprados'#39' ,' +
        'P.COD_PROD,P.DESCRICAO_PROD,F.NOME_FORNECEDOR'
      'FROM FORNECEDORES F'
      'INNER JOIN PRODUTO P ON P.COD_FORNECEDOR = F.COD_FORNECEDOR'
      'INNER JOIN COMPRA_ITENS CI ON CI.COD_PROD = P.COD_PROD'
      'INNER JOIN COMPRA C ON C.COD_COMPRA = CI.COD_COMPRA'
      'GROUP BY P.DESCRICAO_PROD,P.COD_PROD,F.NOME_FORNECEDOR'
      'ORDER BY SUM(CI.QUANTIDADE_ITENS) DESC')
    Left = 136
    Top = 312
    object qTopProdutoProdutosComprados: TIntegerField
      FieldName = 'Produtos Comprados'
      ReadOnly = True
    end
    object qTopProdutoCOD_PROD: TAutoIncField
      FieldName = 'COD_PROD'
      ReadOnly = True
    end
    object qTopProdutoDESCRICAO_PROD: TStringField
      FieldName = 'DESCRICAO_PROD'
      Size = 100
    end
    object qTopProdutoNOME_FORNECEDOR: TStringField
      FieldName = 'NOME_FORNECEDOR'
      Size = 100
    end
  end
end
