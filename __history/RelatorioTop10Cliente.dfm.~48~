  object qTopCliente: TADOQuery
    Connection = TDMPrincipal.Connection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT TOP(10) COUNT(C.COD_CLI)AS '#39'QUANTIDADE DE CLIENTES'#39',SUM(C' +
        'P.VALORTOTAL_COMPRA)AS '#39'VALOR TOTAL'#39
      'FROM CLIENTE C'
      'INNER JOIN COMPRA CP ON CP.COD_CLI = C.COD_CLI')
    Left = 40
    Top = 408
  end
  object dsTopCliente: TDataSource
    DataSet = qTopCliente
    Left = 120
    Top = 408
  end
end
