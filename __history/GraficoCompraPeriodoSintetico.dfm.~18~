  object qCompra: TADOQuery
    Active = True
    Connection = TDMPrincipal.Connection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT CONVERT(VARCHAR(10),DATA_COMPRA,103)AS DATA_COMPRA,SUM(QU' +
        'ANTIDADE_COMPRA)AS QUANTIDADE'
      'FROM COMPRA'
      'GROUP BY DATA_COMPRA'
      'ORDER BY DATA_COMPRA')
    Left = 16
    Top = 304
