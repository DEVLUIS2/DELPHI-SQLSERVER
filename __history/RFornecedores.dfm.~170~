  object qFornecedores: TADOQuery
    Connection = TDMPrincipal.Connection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SELECT '#39'ATIVO'#39' AS STATUS, COUNT(COD_FORNECEDOR)AS COD_FORNECEDOR'
      'FROM FORNECEDORES'
      'WHERE ATIVO = 1'
      'UNION ALL'
      
        'SELECT '#39'INATIVO'#39' AS STATUS, COUNT(COD_FORNECEDOR)AS COD_FORNECED' +
        'OR'
      'FROM FORNECEDORES'
      'WHERE ATIVO = 0')
    Left = 88
    Top = 392
  end
  object dsFornecedores: TDataSource
    DataSet = qFornecedores
    Left = 168
    Top = 392
  end
