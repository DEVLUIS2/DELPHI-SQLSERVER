  end
  object qTopFornecedores: TADOQuery
    Connection = TDMPrincipal.Connection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT TOP 10 SUM(CI.QUANTIDADE_ITENS) AS '#39'Produtos Comprados'#39' ,' +
        'F.COD_FORNECEDOR,P.DESCRICAO_PROD,F.NOME_FORNECEDOR'
      'FROM FORNECEDORES F'
      'INNER JOIN PRODUTO P ON P.COD_FORNECEDOR = F.COD_FORNECEDOR'
      'INNER JOIN COMPRA_ITENS CI ON CI.COD_PROD = P.COD_PROD'
      'INNER JOIN COMPRA C ON C.COD_COMPRA = CI.COD_COMPRA'
      'GROUP BY P.DESCRICAO_PROD,F.COD_FORNECEDOR,F.NOME_FORNECEDOR'
      'ORDER BY SUM(CI.QUANTIDADE_ITENS) DESC')
    Left = 136
    Top = 328
    object qTopFornecedoresProdutosComprados: TIntegerField
      FieldName = 'Produtos Comprados'
      ReadOnly = True
    end
    object qTopFornecedoresCOD_FORNECEDOR: TAutoIncField
      FieldName = 'COD_FORNECEDOR'
      ReadOnly = True
    end
    object qTopFornecedoresDESCRICAO_PROD: TStringField
      FieldName = 'DESCRICAO_PROD'
      Size = 100
    end
    object qTopFornecedoresNOME_FORNECEDOR: TStringField
      FieldName = 'NOME_FORNECEDOR'
      Size = 100
    end
  end
  object dsTopFornecedores: TDataSource
    DataSet = qTopFornecedores
    Left = 264
    Top = 328
  end
end
